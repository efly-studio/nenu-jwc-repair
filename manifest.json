{
    "manifest_version": 2,
    "name": "nenu-jwc-repair",
    "version": "2.0.3",
    "description": "东北师大教务处chrome增强插件",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "<all_urls>"
    ],
    "content_scripts" : [{
        "matches" : ["http://*/*"],
        "js" : ["inject.js"],
        "all_frames" : true ,
        "run_at" : "document_start"
    }],
    "web_accessible_resources": [
        "inline.js"
    ]
}
